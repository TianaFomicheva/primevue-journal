{
  "name": "primevue-journal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.x",
    "jest-environment-jsdom": "^29.6.1",
    "primeflex": "^3.3.1",
    "primevue": "^3.30.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "typescript": "^5.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@testing-library/vue": "^6.4.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/vue3-jest": "^29.2.4",
    "babel-jest": "^29.6.1",
    "jest": "^29.6.1",
    "vite": "^4.4.0"
  },
  "jest": {
    
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "customExportConditions": [
        "node",
        "node-addons"
      ]
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue3-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!primevue/.*)"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
